  <!-- TURN toggle 0 to 1 to force relay call. Protocol either udp or tcp for protocol to use for relay call-->
  <turn force="0" protocol="udp" />

  <!-- Secure call toggling -->
  <secure property="call" value="1" />   <!-- Toggle 1 to 0 for disabling secure call and vice-versa -->

  <!-- Windows Renderer Toggling -->
  <!-- Toggle 1 to 0 for disabling direct3d9 to switch to GDI and vice-versa -->
  <render property="d3d9" value="1" /> 

  <!-- Toggle writing pcap file for media -->
  <!-- Toggle 1 to 0 for not writing pcap packets and vice-versa -->
  <transport property="writepcap" value="0" />

  <!-- Adjust the scale factor for color vividness -->
  <colorVividness chromaScaleFactor="23" />

  <debug>
    <!-- enabled = (1/0) This pumps in media from a pcap in place of remote streams -->
    <!-- pjsip log level = (4-6) increase the level to see more debug prints -->
    <readpcap enabled="0" />
    <pjsiplog level="4" />
  </debug>

  <VP8encoder>
    <!-- VP8 video encoder quality parameters -->
    <!-- Valid range for each of these QP properties is 0 to 63 -->
    <!-- minQP is the minimum (highest) quality parameter for main video -->
    <!-- maxQP is the maximum (lowest) quality parameter for main video -->
    <!-- minQPContent is the minimum (highest) quality parameter for content -->
    <!-- maxQPContent is the maxmium (lowest) quality parameter for content -->
    <QP minQP="4" maxQP="40" minQPContent="4" maxQPContent="47" minQPRDC="4" maxQPRDC="63"/>
    <!-- contentHighResBitrate is the lowest bitrate parameter for content with 1080p resolution-->
    <!-- videoFreeRangeQPBitrate is the lowest bitrate parameter for video with free range qp value-->
    <bitrate contentHighResBitrate="81" videoFreeRangeQPBitrate="1200"/>
  </VP8encoder>

  <!-- Toggle 0 to 1 for enabling sip worker thread -->
  <thread property="sip_cnt" value="0" />

  <RTX>
    <!-- rtx manager related parameters -->
    <!-- maxLowRTT and maxLoss will work together -->
    <!-- if RTT bigger than maxLowRTT and loss bigger than maxLoss, temporal layer will be enabled even rtx is enabled -->
    <RTT maxLowRTT="150" maxLoss="0.01" maxAllowedRTXDelay="350"/>
    <Loss minLossToEnableRtx="0.001" minIntervaleBetweenEnablement="20"/>
  </RTX>

  <audio_gd_params rx_loss="25" rx_tau="40" tx_loss="25" tx_tau="40"/>
</bjn>
